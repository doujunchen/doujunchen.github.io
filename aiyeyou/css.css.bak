*{margin:0;padding:0;border:0;text-decoration:none;list-style-type:none;outline:0;}
#top{
	background:url(images/top.gif) repeat;
	width:100%;height:28px;
    font-size: 11px;
}
#middle{
	background:url(images/bg.gif) repeat;
	width:100%;height:892px;
}
#bottom{
	background:url(images/bg2.gif) repeat;
	width:100%;height:142px;
}
.box{width:957px;height:28px;overflow:hidden;margin:0 auto;line-height:28px;}
.top_left{
	width:500px;height:28px;float:left;color:#f26520;overflow: hidden;
}
.top_left img{
	margin-right:10px;
	float:left;
}
.top_left a{
	color:#f26520;
	height:12px;
	float:left;
}
.top_right a{
	color:black;
}
.ma{
	width:980px;
	height:68px;
	margin:0 auto;
	overflow:hidden;
}
.ma img{
	float:left;
}
.ar{
	width:764px;
	float:left;
	margin-top:14px;
}
.ar a{
	width:94px;
	height:30px;
	border:5px solid #e3e3e5;
	border-radius:50px;
	color:black;
	font-size:14px;
	float:left;
	text-align:center;
	margin-left:18px;
	line-height:30px;
	background-image:-webkit-linear-gradient(bottom,#ffffff,#e8e8ea);
	background-image:-moz-linear-gradient(bottom,#ffffff,#e8e8ea);
}
.ar a:hover{
	background:#febf26;
	color:white;
    border:5px solid #febf26;
	box-shadow:0 6px 0 #f2e3c4;
}
#mb{
	width:980px;
	height:274px;
	margin:5px auto;
	background-image:-webkit-linear-gradient(left top,#f4cb0d,#feb005);
	background-image:-moz-linear-gradient(left top,#f4cb0d,#feb005);
}
.bl{
	width:273px;
	height:252px;
	border:1px solid #e4a61f;
	margin:10px;
	float:left;
	background:#ffe489;inline-block;overflow:hidden;
	margin-right:8px;
}
#mb .bl div{
	height:44px;width:255px;border:1px solid #e9c83b;
	margin:15px 8px 5px;
	border-radius:6px;
	float:left;
}
#mb .bl div span{
	font-size:11px;
	height:34px;width:54px;
	background:white;
	float:left;
	text-align:center;
	line-height:34px;
	margin:5px;border-radius:6px 0 0 6px;
	margin-right:0;
}
#mb .bl div input{
	float:left;height:34px;width:192px;
	margin:5px 0;
	border-radius:0 6px 6px 0;
}
#mb .bl .b1{
	width:109px;height:34px;float:left;margin:12px;
	background:#ff9932;color:white;border:2px solid #e3a828;
	border-radius:6px;cursor: pointer;
}
#mb .bl a{
	color:#825804;font-size:11px;float:right;margin-top:35px;cursor: pointer;
}
#mb .bl .b2{
	width:254px;height:43px;border:1px solid #cc9419;
	background:#fec506;font-size:25px;color:#f4770d;border-radius:6px;
	cursor: pointer;margin-left:10px;
}
#mb .br{
	width:667px;height:253px;margin:11px 9px 7px 0;float:left;display:inline-block;
	
}
#mb .br img{
	width:680px;cursor: pointer;
}
.cl{float:left;}
.cl .clt .tit{
	width:282px;height:29px;background-image:-webkit-linear-gradient(bottom,#f5f3f4,#e8e8e8);
	background-image:-moz-linear-gradient(bottom,#f5f3f4,#e8e8e8);
	border:1px solid #dadada;
}
.cl .clt .tit img{float:left}
.cl .clt .tit b{display:inline-block;line-height:29px;float:left}
.cl .clt .bo{width:282px;height:188px;background:white;}
.cl .clt .bo p{color:#656565;padding:5px 10px;font-size:12px;}
.cl .clt .bo p a{color:#656565;padding-left:5px;font-size:12px;white-space: nowrap;}
.cl .clb .titl{
	width:282px;height:29px;background-image:-webkit-linear-gradient(bottom,#f5f3f4,#e8e8e8);
	background-image:-moz-linear-gradient(bottom,#f5f3f4,#e8e8e8);
	border:1px solid #dadada;
}
.cl .clb .titl img{float:left;margin:8px 9px;}
.cl .clb .titl b{display:inline-block;line-height:29px;float:left}
.cl .clb{margin:16px 0;width:280px;height:200px;border:1px solid #dadadc;
         overflow: hidden;}
.cl .clb .bon{width:280px;height:190px;background-color: #fff;}
.cl .clb .bon p{color:#656565;padding:7px 10px;font-size:12px;}
.cl .clb .bon p a{color:#656565;padding-left:5px;font-size:12px;}
.cr{height:437px;width:687px;float:right;}
.cr .crt{width:684px;height:102px;border:1px solid #ccccce;float:left;background:#fff4ca;}
.cr .crt>div{width:209px;height:90px;border:1px solid #e7c74c;margin:5px 8px;
             background:url(images/1.gif) no-repeat right;float:left;
			 background-color:white;overflow: hidden;}
.cr .crt div p{font-size:12px;padding:5px 0 0 10px;color:#666666;}
.cr .crt div a{font-size:12px;padding-left:10px;color:#ecb330;}
.cr .crt div .jinru{display: block;width:70px;height: 18px;
				border:1px solid #e1e4eb;background-image:-webkit-linear-gradient(left top,#fcfbff,#eaecf9);
	background-image:-moz-linear-gradient(left top,#fcfbff,#eaecf9);padding:0;
				text-align: center;margin:6px 0 0 10px;color:#fe953a}
.cr .crb{width:688px;height:324px;margin-top:12px;float:left;}
.crb .crbl .ti{width:424px;height: 29px;border-bottom: 1px solid #dadada;
				background:url(images/10.gif) no-repeat 10px 9px;overflow: hidden;
				background-color: #eeeced;}
.crb .crbl .ti b{line-height:29px;padding-left:30px;font-size:12px;}
.crb .crbl .ti a{float:right;line-height: 29px;color:#666666;margin-right: 10px;}
.crb .crbl{width: 424px;height: 322px;float:left;border:1px solid #dadada;}
.crb .crbr{width: 248px;height: 322px;float:right;border:1px solid #dadada;}
.crb .crbl .crblb{width:424px;height:292px;background-color: #fff;}
.crb .crbl .crblb div{width: 204px; height: 134px;border:1px solid #ffda9d;
			margin:7px 0 0 4px;float:left;background-color: #fff4ca;overflow: hidden;
			font-size: 12px;color:#6c684f;}
.crb .crbl .crblb div span:nth-child(2){float:left;margin-top:5px;}
.crb .crbl .crblb div span:nth-child(3){float:right;margin-top:5px;}

.crb .crbl .crblb div p{float:left;margin-top:5px;color:#6c684f;}
.crb .crbl .crblb div p a{color:#6c684f;}

.crb .crbl .crblb .last{float:right;width: 70px;height:18px;line-height:18px;
			border:1px solid #e1e4eb;background-image:-webkit-linear-gradient(left top,#fcfbff,#eaecf9);
	background-image:-moz-linear-gradient(left top,#fcfbff,#eaecf9);color:#ff9537;
			margin:3px 5px 0 0;text-align: center;}
.crb .crbr .title{width:248px;height:29px;border-bottom: 1px solid #dadada;overflow: hidden;
			background:url(images/07.gif) no-repeat right top;background-color: #eeeced;}
.crb .crbr .title b{line-height: 29px;padding-left:10px;}
.crb .crbr .bod{width: 248px;height:292px;background-color: #fff;overflow: hidden;}
.crb .crbr .bod p {display: block;width:248px; height:20px;font-size:12px;margin:10px 0;}
.crb .crbr .bod p img{float:left;margin-left:10px;}
.crb .crbr .bod p span{float:right;margin-right:10px;color:#8c8c8e;}
.crb .crbr .bod p a{color:#8c8c8e;}
#bottom{width:978px;height: 88px;margin:0 auto;border:1px solid #dcdadd;
		overflow:hidden;}
#bottom .shang{width:976px;height:29px;border-bottom:1px solid #dcdadd;background-image:-webkit-linear-gradient(bottom,#f5f3f4,#e8e8e8);
	background-image:-moz-linear-gradient(bottom,#f5f3f4,#e8e8e8);}
#bottom .shang b{line-height: 29px;padding-left: 15px;}
#bottom .xia{width:978px;height:58px;background-color: #fff;overflow: hidden;}
#bottom .xia a{font-size: 12px;padding-left:9px;color:#666666;line-height: 29px;}
#fotter{width:100%;height: 140px;margin: 0 auto;margin-top:1px;background:url(images/bg2.gif);}
#fotter p{line-height: 40px;text-align: center;font-size:12px;color:#666668;}
#fotter p a{color:#666668;padding:0 5px;}